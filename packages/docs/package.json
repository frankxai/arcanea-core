{
  "name": "@arcanea/docs",
  "version": "1.0.0",
  "description": "Comprehensive documentation ecosystem for Arcanea AI character platforms",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && vuepress build src",
    "dev": "vuepress dev src",
    "serve": "vuepress serve src",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "generate-api-docs": "typedoc --out dist/api src",
    "generate-examples": "node scripts/generate-examples.js",
    "test-docs": "vitest run src/**/*.test.ts",
    "lint-docs": "markdownlint src/**/*.md",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "keywords": [
    "documentation",
    "arcanea",
    "ai",
    "character",
    "platform",
    "guides",
    "api-docs",
    "tutorials"
  ],
  "author": "Frank X <frank@arcanea.ai>",
  "license": "MIT",
  "dependencies": {
    "@vuepress/core": "^2.0.0",
    "@vuepress/bundler-vite": "^2.0.0",
    "@vuepress/theme-default": "^2.0.0",
    "@vuepress/plugin-search": "^2.0.0",
    "@vuepress/plugin-docsearch": "^2.0.0",
    "vuepress": "^2.0.0",
    "vue": "^3.3.0",
    "typedoc": "^0.25.0",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^5.0.0",
    "redoc": "^2.1.0",
    "prismjs": "^1.29.0",
    "mermaid": "^10.4.0",
    "plantuml": "^0.1.0",
    "markdown-it": "^13.0.0",
    "markdown-it-mermaid": "^0.2.0",
    "markdown-it-plantuml": "^1.4.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.0",
    "katex": "^0.16.0",
    "highlight.js": "^11.8.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/vue3": "^7.4.0",
    "@storybook/vue3-vite": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "markdownlint": "^0.31.0",
    "markdownlint-cli": "^0.37.0",
    "gh-pages": "^6.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0",
    "vite": "^4.4.0"
  }
}